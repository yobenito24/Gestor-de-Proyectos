<h1>Listing stakeholders</h1>

<table class="table table-striped">
  <thead>
  <tr>
    <th>User</th>
    <th>Charter</th>
    <th>Role</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  </thead>
 <tbody>
<% @stakeholders.each do |stakeholder| %>
  <tr>
    <td><%= stakeholder.user %></td>
    <td><%= stakeholder.charter %></td>
    <td><%= stakeholder.role %></td>
    <td><%= link_to 'Show', stakeholder %></td>
    <td><%= link_to 'Edit', edit_stakeholder_path(stakeholder) %></td>
    <td><%= link_to 'Destroy', stakeholder, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
  </tbody>
</table>

<br />

<%= link_to 'New Stakeholder', new_stakeholder_path, :class=>'btn btn-primary' %>
